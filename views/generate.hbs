<p>generator</p>

<form method="get" action="">
    <label for="genre"><b>Select Genre:</b> </label>
    <select class="type" id="genre" name="genre" value="" required>
        <option class="type" value="rock">Rock</option>
        <option class="type" value="pop">Pop</option>
        <option class="type" value="indie-rock">Indie Rock</option>
        <option class="type" value="hip-hop">Hip-Hop</option>
        <option class="type" value="emo-rap">Emo Rap</option>
        <option class="type" value="bedroom-pop">Bedroom Pop</option>
    </select>
    <input type="submit">
</form>

{{#each songs}}
<p>{{this.name}}</p>
{{/each}}